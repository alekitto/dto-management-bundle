{
    "name": "fazland/dto-management",
    "type": "library",
    "description": "DTO finder and param converter bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Massimiliano Braglia",
            "email": "massimiliano.braglia@fazland.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "kcs/class-finder": "^0.1",
        "ocramius/proxy-manager": "^2.1",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/expression-language": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/security": "^4.0"
    },
    "require-dev": {
        "php-mock/php-mock": "^1.0",
        "php-mock/php-mock-prophecy": "^0.0.2",
        "phpunit/phpunit": "^6.5",
        "symfony/browser-kit": "^4.0",
        "symfony/security-bundle": "^4.0",
        "symfony/var-dumper": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Fazland\\DtoManagementBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fazland\\DtoManagementBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
