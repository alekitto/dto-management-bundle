{
    "name": "fazland/dto-management-bundle",
    "type": "library",
    "description": "DTO finder and param converter bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Massimiliano Braglia",
            "email": "massimiliano.braglia@fazland.com"
        },
        {
            "name": "Alessandro Chitolina",
            "email": "alessandro.chitolina@fazland.com"
        },
        {
            "name": "Fazland Dev Team",
            "email": "devs@fazland.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/annotations": "^1.6",
        "kcs/class-finder": "^0.1,>=0.1.2",
        "ocramius/proxy-manager": "^2.1",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/config": "^3.4|^4.0",
        "symfony/framework-bundle": "^3.4|^4.0"
    },
    "require-dev": {
        "kcs/serializer": "2.x-dev",
        "php-mock/php-mock": "^1.0",
        "php-mock/php-mock-prophecy": "^0.0.2",
        "phpunit/phpunit": "^7.4",
        "symfony/browser-kit": "^3.4|^4.0",
        "symfony/expression-language": "^3.4|^4.0",
        "symfony/security-bundle": "^3.4|^4.0",
        "symfony/var-dumper": "^3.4|^4.0",
        "symfony/yaml": "^3.4|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Fazland\\DtoManagementBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fazland\\DtoManagementBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
